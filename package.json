        "mongodb": "^6.9.0",
        "mongoose": "^8.6.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/node": "^20.9.3",
        "concurrently": "^9.0.1",
        "eslint": "^9.11.1",
        "globals": "^15.9.0",
        "pkgroll": "^2.5.0",
        "prettier": "^3.3.3",
